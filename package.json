{
  "name": "crownbot",
  "styled": "CrownBot",
  "description": "A Discord bot that uses Last.fm API to track scrobbles and rank users in a guild.",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "tsc && yarn run copy:fonts",
    "start": "node ./dist/index.js",
    "lint": "yarn run eslint ./src/",
    "dev": "yarn run copy:fonts && tsc -w",
    "copy:fonts": "copyfiles src/fonts/* dist/src/fonts/"
  },
  "main": "./dist/index.js",
  "author": "Shaun",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "copyfiles": "^2.3.0",
    "eslint": "^7.15.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@devraelfreeze/discordjs-pagination": "^2.6.4",
    "@types/chart.js": "^2.9.25",
    "@types/lyricist": "^2.2.0",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.7.18",
    "@types/spotify-web-api-node": "^4.0.2",
    "@types/ws": "^7.2.5",
    "axios": "^0.21.1",
    "canvas": "2.10.2",
    "chart.js": "^2.9.3",
    "chartjs-node-canvas": "^3.0.6",
    "cheerio": "^1.0.0-rc.3",
    "discord.js": "^14.3.0",
    "lyricist": "https://github.com/d-shaun/lyricist.git",
    "moment": "^2.24.0",
    "mongodb": "^4.9.1",
    "mongoose": "^6.6.0",
    "number-abbreviate": "^2.0.0",
    "spotify-web-api-node": "^5.0.0"
  }
}
