{
  "name": "crownbot",
  "styled": "CrownBot",
  "description": "A Discord bot that uses Last.fm API to track scrobbles and rank users in a guild.",
  "scripts": {
    "build": "tsc && yarn run copy:fonts",
    "start": "node ./dist/index.js",
    "lint": "yarn run eslint ./src/",
    "dev": "yarn run copy:fonts && tsc -w",
    "copy:fonts": "copyfiles src/fonts/* dist/src/fonts/"
  },
  "main": "./dist/index.js",
  "author": "Shaun",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "copyfiles": "^2.3.0",
    "eslint": "^7.15.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/chart.js": "^2.9.25",
    "@types/cheerio": "^0.22.21",
    "@types/lyricist": "^2.2.0",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.14",
    "@types/spotify-web-api-node": "^4.0.2",
    "@types/ws": "^7.2.5",
    "axios": "^0.21.1",
    "canvas": "^2.6.1",
    "chart.js": "^2.9.3",
    "chartjs-node-canvas": "^3.0.6",
    "cheerio": "^1.0.0-rc.3",
    "discord-paginationembed": "^2.0.0",
    "discord.js": "^12.5.3",
    "lyricist": "https://github.com/d-shaun/lyricist.git",
    "moment": "^2.24.0",
    "mongoose": "^5.9.20",
    "number-abbreviate": "^2.0.0",
    "spotify-web-api-node": "^5.0.0"
  }
}
