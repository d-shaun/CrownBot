{
  "name": "crownbot",
  "styled": "CrownBot",
  "description": "A Discord bot that uses Last.fm API to track scrobbles and rank users in a guild.",
  "engines": {
    "node": "19.x"
  },
  "scripts": {
    "build": "tsc && yarn run copy:fonts && yarn run copy:html",
    "start": "node ./dist/index.js",
    "lint": "yarn run eslint ./src/",
    "dev": "yarn run copy:fonts && tsc -w",
    "copy:fonts": "copyfiles src/fonts/* dist/",
    "copy:html": "copyfiles html/* dist/"
  },
  "main": "./dist/index.js",
  "author": "Shaun",
  "license": "MIT",
  "devDependencies": {
    "@types/chart.js": "^2.9.25",
    "@types/node": "^18.7.18",
    "@types/spotify-web-api-node": "^4.0.2",
    "@types/ws": "^7.2.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.15.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@devraelfreeze/discordjs-pagination": "^2.7.4",
    "@types/diff": "^5.0.2",
    "axios": "^0.21.1",
    "canvas": "^2.11.2",
    "cheerio": "^1.0.0-rc.12",
    "diff": "^5.1.0",
    "discord.js": "^14.9.0",
    "dotenv": "^16.3.1",
    "moment": "^2.24.0",
    "mongodb": "^4.9.1",
    "mongoose": "^6.6.0",
    "number-abbreviate": "^2.0.0",
    "spotify-web-api-node": "^5.0.2",
    "tslib": "^2.4.1"
  },
  "packageManager": "yarn@3.6.1"
}
